<h1><%= @user.title %>'s Articles</h1>

<ul>
	<% @user.articles.each do |article| %>
		<li><%= link_to article.title, article %></li>
	<% end %>
</ul>

<p><%= link_to 'Edit this user', edit_user_path(@user) %></p>

<p><%= link_to 'All users', users_path %></p>

<p><%= link_to 'All articles', articles_path %></p>

<p><%= link_to 'Delete!', @user, :method => :delete %></p>